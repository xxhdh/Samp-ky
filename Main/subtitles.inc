new issubtitlerunning = 0;

CMD:subtitles(playerid, params[]) {
	if(issubtitlerunning == 1) {
		SCM(playerid, COLOR_RED, "running");
		return 1;
	}
	SubtitlesStartTimer(0);
	return 1;
}

forward SubtitlesStartTimer(id);
public SubtitlesStartTimer(id) {
	if(id >= 0 && id < sizeof Subtitles) {
		issubtitlerunning = 1;
		SCMToAll(COLOR_RED, Subtitles[id][Text]);
		SetTimerEx("SubtitlesStartTimer", Subtitles[id][Timeout], false, "d", id+1);
	} else {
		issubtitlerunning = 0;
	}
}